{
  "name": "subash-portfolio",
  "version": "1.0.0",
  "description": "Modern portfolio website for Subash A - React Frontend Developer",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "build": "cd client && npm install && npm run build",
    "install-all": "npm install && cd client && npm install",
    "seed": "node server/scripts/seedProjects.js"
  },
  "keywords": ["portfolio", "react", "tailwind", "express", "frontend"],
  "author": "Subash A",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "nodemailer": "^6.9.7",
    "concurrently": "^8.2.2",
    "mongoose": "^8.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  }
}
